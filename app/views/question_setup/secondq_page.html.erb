<%= form_for @survey, url: next_wizard_path, method: :put do |f| %>

<!-- need to add question 6 here -->
<h3> 6. Do you feel like you are being punished?</h3>
	<%= f.label :q_six, "I don't feel I am being punished.", value: 0 %>
	<%= f.radio_button :q_six, 0 %>

	<%= f.label :q_six, "I feel I may be punished.", value: 1 %>
	<%= f.radio_button :q_six, 1 %>

	<%= f.label :q_six, "I expect to be punished.", value: 2 %>
	<%= f.radio_button :q_five, 2 %>

	<%= f.label :q_six, "I feel I am being punished.", value: 3 %>
	<%= f.radio_button :q_six, 3 %>


<!-- need to add question 7 here -->
<h3> 7. Are you disappointed in yourself?</h3>

	<%= f.label :q_seven, "I don't feel disappointed in myself.", value: 0 %>
	<%= f.radio_button :q_seven, 0 %>

	<%= f.label :q_seven, "I am disappointed in myself.", value: 1 %>
	<%= f.radio_button :q_seven, 1 %>

	<%= f.label :q_seven, "I am disgusted with myself.", value: 2 %>
	<%= f.radio_button :q_seven, 2 %>

	<%= f.label :q_seven, "I hate myself.", value: 3 %>
	<%= f.radio_button :q_seven, 3 %>


<!-- need to add question 8 here -->
<h3> 8. Are you self critical?</h3>

	<%= f.label :q_eight, "I don't feel I am any worse than anybody else.", value: 0 %>
	<%= f.radio_button :q_eight, 0 %>

	<%= f.label :q_eight, "I am critical of myself for my weaknesses or mistakes.", value: 1 %>
	<%= f.radio_button :q_eight, 1 %>

	<%= f.label :q_eight, "I blame myself all the time for my faults.", value: 2 %>
	<%= f.radio_button :q_eight, 2 %>

	<%= f.label :q_eight, "I blame myself for everything bad that happens.", value: 3 %>
	<%= f.radio_button :q_eight, 3 %>


<!-- need to add question 9 here -->
<h3> 9. Have you ever thought about suicide?</h3>

	<%= f.label :q_nine, "I don't have any thoughts of killing myself.", value: 0 %>
	<%= f.radio_button :q_nine, 0 %>

	<%= f.label :q_nine, "I have thoughts of killing myself, but I would not carry them out.", value: 1 %>
	<%= f.radio_button :q_nine, 1 %>

	<%= f.label :q_nine, "I would like to kill myself.", value: 2 %>
	<%= f.radio_button :q_nine, 2 %>

	<%= f.label :q_nine, "I would kill myself if I had the chance.", value: 3 %>
	<%= f.radio_button :q_nine, 3 %>


<!-- need to add question 10 here -->
<h3> 10. How often do you cry?</h3>

	<%= f.label :q_ten, "I don't cry any more than usual.", value: 0 %>
	<%= f.radio_button :q_ten, 0 %>

	<%= f.label :q_ten, "I cry more now than I used to.", value: 1 %>
	<%= f.radio_button :q_ten, 1 %>

	<%= f.label :q_ten, "I cry all the time now.", value: 2 %>
	<%= f.radio_button :q_ten, 2 %>

	<%= f.label :q_ten, "I used to be able to cry, but now I can't cry even though I want to.", value: 3 %>
	<%= f.radio_button :q_ten, 3 %>

	<%= f.submit "Next" %>

<% end %>
